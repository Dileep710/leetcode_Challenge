import java.util.Scanner;
class Solution
{
    public int fib(int num)
    {
        if(num == 0)
        {
            return 0;
        }
        else if(num == 1)
        {
            return 1;
        }
        int term1 = 0;
        int term2 = 1;
        for(int i = 1; i <= num; i++)
        {
            int term3 = term1 + term2;
            term1 = term2;
            term2 = term3;
        }return term1;    
    }
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the term number (n) to find nth Fibonacci number: ");
        int num = sc.nextInt();
        Solution s = new Solution();
        int result = s.fib(num);
        System.out.println("The " + num + "th Fibonacci number is: " + result);
        sc.close();
    }
}
